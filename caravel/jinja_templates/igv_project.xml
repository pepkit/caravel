<?xml version="1.0" encoding="UTF-8"?>
<Global name="{{ project }}"  infolink="http://www.broadinstitute.org/igv/" version="1">
    <Category name="Category (hg38)">
        <Category name="Subcategory One">
            {% for item in items %}
            <Resource name="{{ item.name }}"
                  path="{{ item.path }}" />
            {% endfor %}
            <Resource name="pi.12mer.tdf"
                  path="http://localhost:5000/data/RRBS_AML_blast_322.bw" />
        </Category>
        <Category name="Subcategory One">
            <Resource name="omega.12mer.tdf"
                  path="http://www.broadinstitute.org/igvdata/annotations/hg18/conservation/omega.12mer.tdf" />
        </Category>
    </Category>
</Global>



{% for dict_item in parent_dict %}
   {% for key, value in dict_item.items() %}
      <h1>Key: {{key}}</h1>
      <h2>Value: {{value}}</h2>
   {% endfor %}
{% endfor %}




{% for pipeline_name, pipeline_outputs in project_outputs %}
    <Category name="{{ pipeline_name }}">
    {% for output_name, output_samples in pipeline_outputs %}
        <Category name="{{ output_name }}">
        {% for sample_name, sample_path in output_samples.items() %}
            <Resource name="{{ sample_name }}" path="{{ sample_path }}">
        {% endfor %}
        </Category>
    {% endfor %}
    </Category>
{% endfor %}
